<p id="notice"><%= notice %></p>

<h1>Uplink B Downlinks</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Uplink</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uplink_b_downlinks.each do |uplink_b_downlink| %>
      <tr>
        <td><%= uplink_b_downlink.value %></td>
        <td><%= uplink_b_downlink.uplink %></td>
        <td><%= link_to 'Show', uplink_b_downlink %></td>
        <td><%= link_to 'Edit', edit_uplink_b_downlink_path(uplink_b_downlink) %></td>
        <td><%= link_to 'Destroy', uplink_b_downlink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

