<p id="notice"><%= notice %></p>

<h1>Time Uplinks</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Uplink</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_uplinks.each do |time_uplink| %>
      <tr>
        <td><%= time_uplink.value %></td>
        <td><%= time_uplink.uplink %></td>
        <td><%= link_to 'Show', time_uplink %></td>
        <td><%= link_to 'Edit', edit_time_uplink_path(time_uplink) %></td>
        <td><%= link_to 'Destroy', time_uplink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Uplink', new_time_uplink_path %>
